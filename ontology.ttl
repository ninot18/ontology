                                                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                   owl:onProperty <https://ninot18.github.io/ontology/#isLocatedIn> ;
                                                                   owl:someValuesFrom <https://ninot18.github.io/ontology/#city>
                                                                 ] ;
                                                 rdfs:comment "A place of interest for tourists such as a museum, monument, park, or beach." ;
                                                 rdfs:label "Attraction" .


