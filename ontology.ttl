#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#creationDate
rdfs:creationDate rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#creator
rdfs:creator rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#license
rdfs:license rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://ninot18.github.io/ontology/#hasAttraction
<https://ninot18.github.io/ontology/#hasAttraction> rdf:type owl:ObjectProperty ;
                                                    rdfs:domain <https://ninot18.github.io/ontology/#city> ;
                                                    rdfs:range <https://ninot18.github.io/ontology/#attraction> ;
                                                    rdfs:comment "Indicates the attractions that are found in a city." ;
                                                    rdfs:label "has attraction" .


###  https://ninot18.github.io/ontology/#hasEvent
<https://ninot18.github.io/ontology/#hasEvent> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <https://ninot18.github.io/ontology/#city> ;
                                               rdfs:range <https://ninot18.github.io/ontology/#event> ;
                                               rdfs:comment "Indicates the events scheduled in the city." ;
                                               rdfs:label "has event" .


###  https://ninot18.github.io/ontology/#hasHotel
<https://ninot18.github.io/ontology/#hasHotel> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <https://ninot18.github.io/ontology/#city> ;
                                               rdfs:range <https://ninot18.github.io/ontology/#hotel> ;
                                               rdfs:comment "Links a city to the hotels it contains." ;
                                               rdfs:label "has hotel" .


###  https://ninot18.github.io/ontology/#hasLatitude
<https://ninot18.github.io/ontology/#hasLatitude> rdf:type owl:ObjectProperty ;
                                                  rdfs:subPropertyOf <https://ninot18.github.io/ontology/#isLocatedIn> ;
                                                  rdfs:range <https://ninot18.github.io/ontology/#city> ;
                                                  rdfs:comment "Latitude of the location." ;
                                                  rdfs:label "has latitude" .


###  https://ninot18.github.io/ontology/#hasLongitude
<https://ninot18.github.io/ontology/#hasLongitude> rdf:type owl:ObjectProperty ;
                                                   rdfs:subPropertyOf <https://ninot18.github.io/ontology/#isLocatedIn> ;
                                                   rdfs:range <https://ninot18.github.io/ontology/#city> ;
                                                   rdfs:comment "Longitude of the location." ;
                                                   rdfs:label "has longitude" .


###  https://ninot18.github.io/ontology/#hasRestaurant
<https://ninot18.github.io/ontology/#hasRestaurant> rdf:type owl:ObjectProperty ;
                                                    rdfs:domain <https://ninot18.github.io/ontology/#city> ;
                                                    rdfs:range <https://ninot18.github.io/ontology/#restaurant> ;
                                                    rdfs:comment "Lists restaurants available in the city." ;
                                                    rdfs:label "has restaurant" .


###  https://ninot18.github.io/ontology/#hasTransport
<https://ninot18.github.io/ontology/#hasTransport> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain <https://ninot18.github.io/ontology/#city> ;
                                                   rdfs:range <https://ninot18.github.io/ontology/#transport> ;
                                                   rdfs:comment "Indicates transport options available in the city." ;
                                                   rdfs:label "has transport" .


###  https://ninot18.github.io/ontology/#isLocatedIn
<https://ninot18.github.io/ontology/#isLocatedIn> rdf:type owl:ObjectProperty ;
                                                  rdfs:range <https://ninot18.github.io/ontology/#city> ;
                                                  rdfs:comment "Relates a tourist resource to the city where it is located." ;
                                                  rdfs:label "is located in" .


