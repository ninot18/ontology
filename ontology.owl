<?xml version="1.0"?>
<rdf:RDF xmlns="https://ninot18.github.io/ontology/"
     xml:base="https://ninot18.github.io/ontology/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:ontology="https://ninot18.github.io/ontology/#">
    <owl:Ontology rdf:about="https://ninot18.github.io/ontology/">
        <owl:versionIRI rdf:resource="https://ninot18.github.io/ontology/1.0.0"/>
        <rdfs:comment>Ontology developed by Pedro Giovanny Rojas Gil</rdfs:comment>
        <rdfs:label>SmartTourism</rdfs:label>
        <owl:versionInfo>Version 1.0.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://ninot18.github.io/ontology/#hasAttraction -->


    <owl:ObjectProperty rdf:about="https://ninot18.github.io/ontology/#hasAttraction">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#city"/>
        <rdfs:range rdf:resource="https://ninot18.github.io/ontology/#attraction"/>
        <rdfs:comment>Indicates the attractions that are found in a city.</rdfs:comment>
        <rdfs:label>has attraction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasEvent -->


    <owl:ObjectProperty rdf:about="https://ninot18.github.io/ontology/#hasEvent">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#city"/>
        <rdfs:range rdf:resource="https://ninot18.github.io/ontology/#event"/>
        <rdfs:comment>Indicates the events scheduled in the city.</rdfs:comment>
        <rdfs:label>has event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasHotel -->


    <owl:ObjectProperty rdf:about="https://ninot18.github.io/ontology/#hasHotel">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#city"/>
        <rdfs:range rdf:resource="https://ninot18.github.io/ontology/#hotel"/>
        <rdfs:comment>Links a city to the hotels it contains.</rdfs:comment>
        <rdfs:label>has hotel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasLatitude -->


    <owl:ObjectProperty rdf:about="https://ninot18.github.io/ontology/#hasLatitude">
        <rdfs:subPropertyOf rdf:resource="https://ninot18.github.io/ontology/#isLocatedIn"/>
        <rdfs:range rdf:resource="https://ninot18.github.io/ontology/#city"/>
        <rdfs:comment>Latitude of the location.</rdfs:comment>
        <rdfs:label>has latitude</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasLongitude -->


    <owl:ObjectProperty rdf:about="https://ninot18.github.io/ontology/#hasLongitude">
        <rdfs:subPropertyOf rdf:resource="https://ninot18.github.io/ontology/#isLocatedIn"/>
        <rdfs:range rdf:resource="https://ninot18.github.io/ontology/#city"/>
        <rdfs:comment>Longitude of the location.</rdfs:comment>
        <rdfs:label>has longitude</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasRestaurant -->


    <owl:ObjectProperty rdf:about="https://ninot18.github.io/ontology/#hasRestaurant">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#city"/>
        <rdfs:range rdf:resource="https://ninot18.github.io/ontology/#restaurant"/>
        <rdfs:comment>Lists restaurants available in the city.</rdfs:comment>
        <rdfs:label>has restaurant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasTransport -->


    <owl:ObjectProperty rdf:about="https://ninot18.github.io/ontology/#hasTransport">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#city"/>
        <rdfs:range rdf:resource="https://ninot18.github.io/ontology/#transport"/>
        <rdfs:comment>Indicates transport options available in the city.</rdfs:comment>
        <rdfs:label>has transport</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://ninot18.github.io/ontology/#isLocatedIn -->


    <owl:ObjectProperty rdf:about="https://ninot18.github.io/ontology/#isLocatedIn">
        <rdfs:range rdf:resource="https://ninot18.github.io/ontology/#city"/>
        <rdfs:comment>Relates a tourist resource to the city where it is located.</rdfs:comment>
        <rdfs:label>is located in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://ninot18.github.io/ontology/#hasAttractionRating -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasAttractionRating">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#attraction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Numerical evaluation score given to a attraction, usually between 0 and 10.</rdfs:comment>
        <rdfs:label>has attraction rating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasAveragePrice -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasAveragePrice">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Average meal price at a restaurant.</rdfs:comment>
        <rdfs:label>has average price</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasCardinalLocation -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasCardinalLocation">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#city"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Geographic latitude and longitude coordinates of a city.</rdfs:comment>
        <rdfs:label>has cardinal coordinates</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasCompany -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasCompany">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The company that provides the transport service.</rdfs:comment>
        <rdfs:label>has company</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasCuisineType -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasCuisineType">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the type of cuisine served by a restaurant.</rdfs:comment>
        <rdfs:label>has cuisine type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasDate -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasDate">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The date on which an event takes place.</rdfs:comment>
        <rdfs:label>has date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasHotelRating -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasHotelRating">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Numerical evaluation score given to a hotel, usually between 0 and 100.</rdfs:comment>
        <rdfs:label>has hotel rating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasId -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasId">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>The unique identification of the resource.</rdfs:comment>
        <rdfs:label>has ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasName -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasName">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>he name of the resource.</rdfs:comment>
        <rdfs:label>has name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasPrice -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasPrice">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>The cost of using a transport service</rdfs:comment>
        <rdfs:label>has price</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasPricePerNight -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasPricePerNight">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>The price for one night of stay at a hotel.</rdfs:comment>
        <rdfs:label>has price per night</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasRating -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasRating">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Numerical evaluation score given to a restaurant, usually between 0 and 10.</rdfs:comment>
        <rdfs:label>hasRating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://ninot18.github.io/ontology/#hasStars -->


    <owl:DatatypeProperty rdf:about="https://ninot18.github.io/ontology/#hasStars">
        <rdfs:domain rdf:resource="https://ninot18.github.io/ontology/#hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>Star rating assigned to a hotel.</rdfs:comment>
        <rdfs:label>has stars</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://ninot18.github.io/ontology/#attraction -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#attraction">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ninot18.github.io/ontology/#isLocatedIn"/>
                <owl:someValuesFrom rdf:resource="https://ninot18.github.io/ontology/#city"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A place of interest for tourists such as a museum, monument, park, or beach.</rdfs:comment>
        <rdfs:label>Attraction</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#beachAttraction -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#beachAttraction">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#attraction"/>
        <rdfs:comment>A sandy or pebbly shore along a sea, lake, or river, often used for recreation.</rdfs:comment>
        <rdfs:label>Beach Attraction</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#beachHotel -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#beachHotel">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#hotel"/>
        <rdfs:comment>A hotel located near the beach, often catering to holidaymakers.</rdfs:comment>
        <rdfs:label>Beach Hotel</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#bicycle -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#bicycle">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#transport"/>
        <rdfs:comment>A two-wheeled vehicle powered by pedaling, used for personal or rental transport.</rdfs:comment>
        <rdfs:label>Bicycle</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#bus -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#bus">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#transport"/>
        <rdfs:comment>A large motor vehicle that carries passengers by road, typically on a fixed route.</rdfs:comment>
        <rdfs:label>Bus</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#business -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#business">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#hotel"/>
        <rdfs:comment>A hotel designed for business travelers, offering amenities such as meeting rooms and internet.</rdfs:comment>
        <rdfs:label>Business Hotel</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#cheapOption -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#cheapOption">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#metro"/>
        <rdfs:comment>A metro that costs less than 1â‚¬.</rdfs:comment>
        <rdfs:label>cheapOption</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#city -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#city">
        <rdfs:comment>A geographical area that contains tourist resources such as attractions, events, hotels, restaurants, and transport.</rdfs:comment>
        <rdfs:label>City</rdfs:label>
        <rdfs:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/ontology/City</rdfs:sameAs>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#culturalAttraction -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#culturalAttraction">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#attraction"/>
        <rdfs:comment>A tourist site that reflects the cultural identity or heritage of a location.</rdfs:comment>
        <rdfs:label>Cultural Attraction</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#culturalEvent -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#culturalEvent">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#event"/>
        <rdfs:comment>An event showcasing local or global cultural expressions like theater, dance, or art.</rdfs:comment>
        <rdfs:label>Cultural Event</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#event -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ninot18.github.io/ontology/#isLocatedIn"/>
                <owl:someValuesFrom rdf:resource="https://ninot18.github.io/ontology/#city"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An organized occurrence that takes place in a city, such as a concert, gastronomic
, or sports event.</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#gastronomic -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#gastronomic">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#event"/>
        <rdfs:comment>An event focused on food tasting, culinary contests, or local cuisine exhibitions.</rdfs:comment>
        <rdfs:label>Gastronomic Event</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#highRatedLuxury -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#highRatedLuxury">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#luxury"/>
        <owl:disjointWith rdf:resource="https://ninot18.github.io/ontology/#lowRatedLuxury"/>
        <rdfs:comment>Luxury Hotels with a rating higher than 90.</rdfs:comment>
        <rdfs:label>highRatedLuxury</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#highRatedRestaurant -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#highRatedRestaurant">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#restaurant"/>
        <owl:disjointWith rdf:resource="https://ninot18.github.io/ontology/#lowRatedRestaurant"/>
        <rdfs:comment>Restaurants wtih 4 or more in ratings.</rdfs:comment>
        <rdfs:label>highRatedRestaurant</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#hotel -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#hotel">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ninot18.github.io/ontology/#isLocatedIn"/>
                <owl:someValuesFrom rdf:resource="https://ninot18.github.io/ontology/#city"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A commercial establishment offering lodging, meals, and other guest services.</rdfs:comment>
        <rdfs:label>Hotel</rdfs:label>
        <rdfs:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/ontology/Hotel</rdfs:sameAs>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#lowRatedLuxury -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#lowRatedLuxury">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#luxury"/>
        <rdfs:comment>Luxury Hotel with a rating lower than 90.</rdfs:comment>
        <rdfs:label>lowRatedLuxury</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#lowRatedRestaurant -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#lowRatedRestaurant">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#restaurant"/>
        <rdfs:comment>Restaurants wtih less than 4 in ratings.</rdfs:comment>
        <rdfs:label>lowRatedRestaurant</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#luxury -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#luxury">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#hotel"/>
        <rdfs:comment>A high-end hotel offering premium services and accommodations.</rdfs:comment>
        <rdfs:label>Luxury Hotel</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#metro -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#metro">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#transport"/>
        <rdfs:comment>An urban rapid transit system using underground or elevated railways.</rdfs:comment>
        <rdfs:label>Metro</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#monument -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#monument">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#attraction"/>
        <rdfs:comment>A structure built to commemorate a notable person or event.</rdfs:comment>
        <rdfs:label>Monument Attraction</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#museum -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#museum">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#attraction"/>
        <rdfs:comment>A building where objects of historical, scientific, or artistic interest are stored and exhibited.</rdfs:comment>
        <rdfs:label>Museum Attraction</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#music -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#music">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#event"/>
        <rdfs:comment>An event where live or recorded music is performed.</rdfs:comment>
        <rdfs:label>Music Event</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#park -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#park">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#attraction"/>
        <rdfs:comment>A public green area in a town, used for recreation.</rdfs:comment>
        <rdfs:label>Park Attraction</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#restaurant -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#restaurant">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ninot18.github.io/ontology/#isLocatedIn"/>
                <owl:someValuesFrom rdf:resource="https://ninot18.github.io/ontology/#city"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A place where meals are prepared and served to customers.</rdfs:comment>
        <rdfs:label>Restaurant</rdfs:label>
        <rdfs:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/ontology/Restaurant</rdfs:sameAs>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#rural -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#rural">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#hotel"/>
        <rdfs:comment>A hotel located in a countryside or natural environment, often offering quiet and nature-based experiences.</rdfs:comment>
        <rdfs:label>Rural Hotel</rdfs:label>
    </owl:Class>
    


    <!-- https://ninot18.github.io/ontology/#sports -->


    <owl:Class rdf:about="https://ninot18.github.io/ontology/#sports">
        <rdfs:subClassOf rdf:resource="https://ninot18.github.io/ontology/#event"/>
        <rdfs:comment>An event that involves physical competition and spectators, such as a football match.</rdfs:comment>
        <rdfs:label>Sports Event</rdfs:label>
