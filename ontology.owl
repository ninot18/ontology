<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#"
     xml:base="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:smart_tourism="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism">
        <rdfs:comment>Ontology developed by Pedro Giovanny Rojas Gil</rdfs:comment>
        <owl:versionInfo>Version 1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasAttraction -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasAttraction">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#attraction"/>
        <rdfs:comment>Indicates the attractions that are found in a city.</rdfs:comment>
        <rdfs:label>has attraction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasEvent -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasEvent">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#event"/>
        <rdfs:comment>Indicates the events scheduled in the city.</rdfs:comment>
        <rdfs:label>has event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasHotel -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasHotel">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hotel"/>
        <rdfs:comment>Links a city to the hotels it contains.</rdfs:comment>
        <rdfs:label>has hotel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasLatitude -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasLatitude">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#isLocatedIn"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
        <rdfs:comment>Latitude of the location.</rdfs:comment>
        <rdfs:label>has latitude</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasLongitude -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasLongitude">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#isLocatedIn"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
        <rdfs:comment>Longitude of the location.</rdfs:comment>
        <rdfs:label>has longitude</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasRestaurant -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasRestaurant">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#restaurant"/>
        <rdfs:comment>Lists restaurants available in the city.</rdfs:comment>
        <rdfs:label>has restaurant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasTransport -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasTransport">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#transport"/>
        <rdfs:comment>Indicates transport options available in the city.</rdfs:comment>
        <rdfs:label>has transport</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#isLocatedIn -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#isLocatedIn">
        <rdfs:range rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
        <rdfs:comment>Relates a tourist resource to the city where it is located.</rdfs:comment>
        <rdfs:label>is located in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasAttractionRating -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasAttractionRating">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#attraction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Numerical evaluation score given to a attraction, usually between 0 and 10.</rdfs:comment>
        <rdfs:label>has attraction rating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasAveragePrice -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasAveragePrice">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Average meal price at a restaurant.</rdfs:comment>
        <rdfs:label>has average price</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasCardinalLocation -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasCardinalLocation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Geographic latitude and longitude coordinates of a city.</rdfs:comment>
        <rdfs:label>has cardinal coordinates</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasCompany -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasCompany">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The company that provides the transport service.</rdfs:comment>
        <rdfs:label>has company</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasCuisineType -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasCuisineType">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Specifies the type of cuisine served by a restaurant.</rdfs:comment>
        <rdfs:label>has cuisine type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasDate -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasDate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The date on which an event takes place.</rdfs:comment>
        <rdfs:label>has date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasHotelRating -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasHotelRating">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Numerical evaluation score given to a hotel, usually between 0 and 100.</rdfs:comment>
        <rdfs:label>has hotel rating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasId -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasId">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>The unique identification of the resource.</rdfs:comment>
        <rdfs:label>has ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasName -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasName">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>he name of the resource.</rdfs:comment>
        <rdfs:label>has name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasPrice -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasPrice">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>The cost of using a transport service</rdfs:comment>
        <rdfs:label>has price</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasPricePerNight -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasPricePerNight">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>The price for one night of stay at a hotel.</rdfs:comment>
        <rdfs:label>has price per night</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasRating -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasRating">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Numerical evaluation score given to a restaurant, usually between 0 and 10.</rdfs:comment>
        <rdfs:label>hasRating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasStars -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hasStars">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>Star rating assigned to a hotel.</rdfs:comment>
        <rdfs:label>has stars</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#attraction -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#attraction">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#isLocatedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A place of interest for tourists such as a museum, monument, park, or beach.</rdfs:comment>
        <rdfs:label>Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#beachAttraction -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#beachAttraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#attraction"/>
        <rdfs:comment>A sandy or pebbly shore along a sea, lake, or river, often used for recreation.</rdfs:comment>
        <rdfs:label>Beach Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#beachHotel -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#beachHotel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hotel"/>
        <rdfs:comment>A hotel located near the beach, often catering to holidaymakers.</rdfs:comment>
        <rdfs:label>Beach Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#bicycle -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#bicycle">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#transport"/>
        <rdfs:comment>A two-wheeled vehicle powered by pedaling, used for personal or rental transport.</rdfs:comment>
        <rdfs:label>Bicycle</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#bus -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#bus">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#transport"/>
        <rdfs:comment>A large motor vehicle that carries passengers by road, typically on a fixed route.</rdfs:comment>
        <rdfs:label>Bus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#business -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#business">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hotel"/>
        <rdfs:comment>A hotel designed for business travelers, offering amenities such as meeting rooms and internet.</rdfs:comment>
        <rdfs:label>Business Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#cheapOption -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#cheapOption">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#metro"/>
        <rdfs:comment>A metro that costs less than 1€.</rdfs:comment>
        <rdfs:label>cheapOption</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city">
        <rdfs:comment>A geographical area that contains tourist resources such as attractions, events, hotels, restaurants, and transport.</rdfs:comment>
        <rdfs:label>City</rdfs:label>
        <rdfs:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/ontology/City</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#culturalAttraction -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#culturalAttraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#attraction"/>
        <rdfs:comment>A tourist site that reflects the cultural identity or heritage of a location.</rdfs:comment>
        <rdfs:label>Cultural Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#culturalEvent -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#culturalEvent">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#event"/>
        <rdfs:comment>An event showcasing local or global cultural expressions like theater, dance, or art.</rdfs:comment>
        <rdfs:label>Cultural Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#event -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#isLocatedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An organized occurrence that takes place in a city, such as a concert, gastronomic
, or sports event.</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#gastronomic -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#gastronomic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#event"/>
        <rdfs:comment>An event focused on food tasting, culinary contests, or local cuisine exhibitions.</rdfs:comment>
        <rdfs:label>Gastronomic Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#highRatedLuxury -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#highRatedLuxury">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#luxury"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#lowRatedLuxury"/>
        <rdfs:comment>Luxury Hotels with a rating higher than 90.</rdfs:comment>
        <rdfs:label>highRatedLuxury</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#highRatedRestaurant -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#highRatedRestaurant">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#restaurant"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#lowRatedRestaurant"/>
        <rdfs:comment>Restaurants wtih 4 or more in ratings.</rdfs:comment>
        <rdfs:label>highRatedRestaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hotel -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#hotel">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#isLocatedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#city"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A commercial establishment offering lodging, meals, and other guest services.</rdfs:comment>
        <rdfs:label>Hotel</rdfs:label>
        <rdfs:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/ontology/Hotel</rdfs:sameAs>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#lowRatedLuxury -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#lowRatedLuxury">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#luxury"/>
        <rdfs:comment>Luxury Hotel with a rating lower than 90.</rdfs:comment>
        <rdfs:label>lowRatedLuxury</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#lowRatedRestaurant -->


    <owl:Class rdf:about="http://www.semanticweb.org/peter/ontologies/2025/3/smart_tourism#lowRatedRestaurant">
